{
    "compilerOptions": {
        "target": "es2020",
        "lib": ["es2020"],
        "strict": false,
        "module": "CommonJS",
        "moduleResolution": "node",
        "sourceMap": true,
        "declaration": true,
        "esModuleInterop": true,
        "outDir": "dist/",
        "plugins": [
            {
                "name": "frida-tsplugin"
            }
        ]
    },
    "include": [
        "**/*.ts"
    ]
}